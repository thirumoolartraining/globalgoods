{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/(assets|images|data)/(.*)", "dest": "/$1/$2", "headers": { "cache-control": "public, max-age=3600" } },
    { "src": "/favicon\\.ico", "dest": "/favicon.ico" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
